{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-multi-spaces": ["error", { "ignoreEOLComments": true }],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }]
  }
}